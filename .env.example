## Copy to .env.local and fill in values.
## PowerShell:
##   Copy-Item .env.example .env.local

# Evolink / Apimart (if you use it)
APIMART_API_KEY=
APIMART_BASE_URL=https://api.evolink.ai/v1
APIMART_MODEL=kimi-k2-thinking
# Quick-start (optional): use faster Kimi K2 Thinking Turbo for quick-* agents
# If not set, it will reuse APIMART_* above.
APIMART_QUICK_API_KEY=
APIMART_QUICK_BASE_URL=
APIMART_QUICK_MODEL=kimi-k2-thinking-turbo

# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Optional: support email shown on pricing/contact
NEXT_PUBLIC_SUPPORT_EMAIL=
# Credits / anti-abuse (hash IP on server; set a long random secret in production)
CREDITS_IP_SALT=

# Supabase (server-only)
SUPABASE_SERVICE_ROLE_KEY=

# WeChat Pay v3 (Native)
WECHATPAY_APPID=
WECHATPAY_MCHID=
WECHATPAY_SERIAL_NO=
# Merchant private key PEM; store as a single line with \n for newlines
WECHATPAY_PRIVATE_KEY=
# API v3 key (32 chars)
WECHATPAY_API_V3_KEY=

# WeChat Pay callback verifier (choose one mode)
# Option A (recommended): WeChat Pay public key mode
WECHATPAY_PLATFORM_PUBLIC_KEY_ID=
# WeChat Pay public key PEM; store as a single line with \n for newlines
WECHATPAY_PLATFORM_PUBLIC_KEY_PEM=

# Option B: Platform certificate mode (legacy)
WECHATPAY_PLATFORM_SERIAL_NO=
# Platform certificate PEM; store as a single line with \n for newlines
WECHATPAY_PLATFORM_CERT_PEM=

# Public HTTPS callback URL, e.g. https://www.example.com/api/wechatpay/notify
WECHATPAY_NOTIFY_URL=
